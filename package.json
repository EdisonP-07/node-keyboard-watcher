{
  "name": "node-keyboard-watcher",
  "version": "1.0.3",
  "author": {
    "name": "Coust"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EdisonP-07/node-keyboard-watcher.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "gypfile": true,
  "scripts": {
    "install": "node-gyp rebuild && yarn build-ts",
    "build32": "node-gyp clean configure build --arch=ia32",
    "build64": "node-gyp clean configure build --arch=x64",
    "prebuild": "prebuildify --napi",
    "build-ts": "tsc",
    "demo": "node-gyp build && yarn build-ts && node dist/demo/keyboardWatcher-demo.js"
  },
  "files": [
    "dist/index.d.ts",
    "dist/index.js",
    "dist/index.js.map",
    "binding.gyp",
    "src/lib",
    "prebuilds"
  ],
  "devDependencies": {
    "prebuildify": "^4.0.0",
    "ts-node": "9.1.1",
    "typescript": "4.9.5",          
    "@types/node": "20.5.1"       
  },
  "dependencies": {
    "@types/throttle-debounce": "2.1.0",  
    "bindings": "^1.5.0",
    "node-addon-api": "^3.2.1",
    "throttle-debounce": "2.1.0"
  }
}
